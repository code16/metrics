{
    "name": "code16/metrics",
    "description": "Metrics Package for Laravel 5",
    "keywords": ["analytics", "metrics", "laravel"],
    "homepage": "https://code16.fr",
    "authors": [
        {
            "name": "RÃ©mi Collin",
            "email": "remi@code16.fr"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "jenssegers/agent": "^2.3",
        "doctrine/dbal": "^2.5",
        "code16/write-to-console": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0|~7.0",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*",
        "mockery/mockery": "0.9.*",
        "fzaninotto/faker": "~1.4",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
       "psr-4": {
         "Code16\\Metrics\\": "src/Metrics"
        },
        "classmap": [
            "src/database"
        ],
        "files": [
            "src/Metrics/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Code16\\Metrics\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Code16\\Metrics\\MetricServiceProvider"
            ]
        }
    },
    "license": "MIT"
    
}
