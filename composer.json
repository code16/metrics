{
    "name": "code16/metrics",
    "description": "Metrics Package for Laravel",
    "keywords": ["analytics", "metrics", "laravel"],
    "homepage": "https://code16.fr",
    "authors": [
        {
            "name": "RÃ©mi Collin",
            "email": "remi@code16.fr"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "illuminate/support": "^11.0",
        "jenssegers/agent": "^2.3",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "9.*",
        "mockery/mockery": "^1.0",
        "fakerphp/faker": "^1.19.0",
        "symfony/css-selector": "5.0.*",
        "symfony/dom-crawler": "5.0.*"
    },
    "autoload": {
       "psr-4": {
         "Code16\\Metrics\\": "src/Metrics"
        },
        "classmap": [
            "src/database"
        ],
        "files": [
            "src/Metrics/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Code16\\Metrics\\Tests\\": "tests/",
            "Code16\\Metrics\\Tests\\Stubs\\": "tests/stubs/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Code16\\Metrics\\MetricServiceProvider"
            ]
        }
    },
    "config": {
        "discard-changes": true
    },
    "license": "MIT"

}
